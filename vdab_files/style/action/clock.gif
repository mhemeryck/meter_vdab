<script type="text/javascript" charset="utf-8">
	jQuery(function() {

		checkInUsePage('#searchFieldPage');

		jQuery(".swap_value_page").each(function(i){
			jQuery(this).focus(function(){
				if (jQuery(this).val() == 'Zoek op deze site') {
					jQuery(this).val("");
					checkInUsePage('#searchFieldPage');
				}
			}).blur(function(){
				if (jQuery.trim(jQuery(this).val()) == "") {
					jQuery(this).val('Zoek op deze site');
				}
				checkInUsePage('#searchFieldPage');
			});
		});

		function checkInUsePage(objId) {
			if (jQuery.trim(jQuery(objId).val()) != 'Zoek op deze site') {
				jQuery(objId).addClass('inUse');
			} else {
				jQuery(objId).removeClass('inUse');
			}
		}

	});
</script>

